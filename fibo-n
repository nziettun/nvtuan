package nhom7;

public class fibo {

	public static void main(String[] args) {
        int n = 11;
        System.out.format("%dth Fibonacci is: %d", n, f(n));
 
    }
    public static int f(int n){
        if(n==1 || n==2){
            return 1;
        }
        else{
            return(f(n-2) + f(n-1));
        }
	}
}
